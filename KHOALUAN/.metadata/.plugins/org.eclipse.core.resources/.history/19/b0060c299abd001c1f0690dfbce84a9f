package cntt.trang.bean;

public class DonVi {
	private long maDonVi;
	private String tenDonVi;
	private long maMuc;
	private String Type;
	public long getMaDonVi() {
		return maDonVi;
	}
	public void setMaDonVi(long maDonVi) {
		this.maDonVi = maDonVi;
	}
	public String getTenDonVi() {
		return tenDonVi;
	}
	public void setTenDonVi(String tenDonVi) {
		this.tenDonVi = tenDonVi;
	}
	public long getMaMuc() {
		return maMuc;
	}
	public void setMaMuc(long maMuc) {
		this.maMuc = maMuc;
	}
	public String getType() {
		return Type;
	}
	public void setType(String type) {
		Type = type;
	}
	public DonVi(long maDonVi, String tenDonVi, long maMuc, String type) {
		super();
		this.maDonVi = maDonVi;
		this.tenDonVi = tenDonVi;
		this.maMuc = maMuc;
		Type = type;
	}
	public DonVi() {
		super();
		// TODO Auto-generated constructor stub
	}
	
}
